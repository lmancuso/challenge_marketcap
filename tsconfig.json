{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",
    "lib": ["ES2016"],
    "module": "NodeNext",
    "rootDir": "src",
    "resolveJsonModule": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true
  }
}
